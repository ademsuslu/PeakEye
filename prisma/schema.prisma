generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Newsletter {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  createdAt DateTime @default(now())
}

model Blogs {
  id         Int      @id @default(autoincrement())
  category   String
  desc       String
  img        String
  userAvatar String
  userName   String
  useeDate   String
  createdAt  DateTime @default(now())
}
